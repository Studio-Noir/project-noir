            <kind>1</kind>
            <string>///Initialize move
x_distance = 550
y_distance = 200

//check collision
dir = sign(obj_char.x - x) // + = right; - = left 
if not place_meeting(self.x+(dir*h_speed*stun_speed_modifier),self.y,obj_inv_blc){
    stop = 0
}else{
    stop = 1
}

//Change speed based on whether or not enemy is stunned
if(stun == 0){
    stun_speed_modifier = 1;
}else{
    stun_speed_modifier = 0.2;
}

//Move towards player
if (stop == 0 and stun == 0){
    if (x - obj_char.x &lt;= x_distance and obj_char.x - x &lt;= x_distance - 50){
        if(abs(y - obj_char.y) &lt;= y_distance){
            x+= sign(obj_char.x-x)*h_speed*stun_speed_modifier;
            sprite_index = spr_enemy2_move
        }else sprite_index = spr_enemy2
    }else sprite_index = spr_enemy2
}else sprite_index = spr_enemy2
    
</string>
