        draw_text(x1 + 1, y1, text);
        draw_text(x1 - 1, y1 + 1, text);
        draw_text(x1, y1 + 1, text);
        draw_text(x1 + 1, y1 + 1, text);
    } else {
        draw_set_blend_mode(bm_subtract);
        draw_set_alpha(alpha / 10);
        draw_ellipse_color(xvar - text_width, yvar - 2 * text_height, xvar + 2 * text_width, yvar + 3 * text_height, c_gray, c_black, false);
        draw_set_alpha(alpha);
        draw_set_blend_mode(bm_normal);
    }
    
    // write out text
    draw_set_font(font_chat);
    draw_set_color(color);
    draw_text(xvar, yvar, text);
    draw_set_alpha(1);
}
